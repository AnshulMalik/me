{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": 2,
    "no-compare-neg-zero": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-irregular-whitespace": 2,
    "no-unreachable": 2,
    "array-callback-return": 1,
    "block-scoped-var": 1,
    "eqeqeq": 2,
    "no-eval": 1,
    "no-multi-spaces": 2,
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "camelcase": 1,
    "comma-spacing": 2,
    "newline-before-return": 2,
    "no-multiple-empty-lines": 2,
    "no-trailing-spaces": 2,
    "space-infix-ops": 2,
    "spaced-comment": 1,
    "constructor-super": 2,
    "no-const-assign": 2,
    "no-var": 2
  }
}
